{
  "name": "preact-www",
  "version": "0.2.0",
  "description": "Preact website.",
  "scripts": {
    "dev": "NODE_ENV=development webpack-dev-server --inline --hot",
    "start": "superstatic build -p ${PORT:-8080} --host 0.0.0.0 --gzip -c '{\"rewrites\": [{\"source\":\"**\",\"destination\":\"index.html\"}],\"headers\":[{\"source\":\"**\",\"headers\":[{\"key\":\"Cache-Control\",\"value\":\"max-age=86400\"}]}]}'",
    "prestart": "npm run build",
    "build": "NODE_ENV=production webpack",
    "prebuild": "npm run -s protect && rm -rf build && mkdir -p build && ncp src/manifest.json build/manifest.json && ncp src/assets build/assets && ncp content build/content",
    "test": "npm run -s lint && npm run -s test:karma",
    "test:karma": "karma start test/karma.conf.js --single-run",
    "protect": "[ \"$NODE_ENV\" = \"production\" ] && snyk protect || exit 0",
    "lint": "eslint {src,test}",
    "predeploy": "npm run -s build",
    "deploy": "cp build/index.html build/200.html && surge -d preactjs.com build"
  },
  "keywords": [
    "preact",
    "boilerplate",
    "webpack"
  ],
  "license": "MIT",
  "author": "Jason Miller <jason@developit.ca>",
  "devDependencies": {
    "autoprefixer": "^6.4.1",
    "babel": "^6.5.2",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-jsx": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-minimal": "^2.0.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "babel-runtime": "^6.6.1",
    "chai": "^3.5.0",
    "core-js": "^2.2.1",
    "css-loader": "^0.24.0",
    "ejs-loader": "^0.3.0",
    "eslint": "^3.1.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "json-loader": "^0.5.4",
    "karma": "^1.1.1",
    "karma-chai": "^0.1.0",
    "karma-chai-sinon": "^0.1.5",
    "karma-mocha": "^1.1.1",
    "karma-mocha-reporter": "^2.0.4",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "less": "^2.6.1",
    "less-loader": "^2.2.3",
    "minimatch": "^3.0.2",
    "mocha": "^3.0.2",
    "ncp": "^2.0.0",
    "offline-plugin": "^3.4.2",
    "phantomjs-prebuilt": "^2.1.7",
    "postcss-loader": "^0.11.1",
    "progress-bar-webpack-plugin": "^1.6.0",
    "raw-loader": "^0.5.1",
    "recursive-readdir-sync": "^1.0.6",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "superstatic": "^4.0.3",
    "surge": "^0.18.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1",
    "worker-loader": "^0.7.1"
  },
  "dependencies": {
    "babel-runtime": "^6.6.1",
    "babel-standalone": "^6.10.3",
    "bootstrap": "^3.3.6",
    "classnames": "^2.2.3",
    "codemirror": "^5.16.0",
    "decko": "^1.1.3",
    "highlight.js": "^9.5.0",
    "isomorphic-fetch": "^2.2.1",
    "marked": "^0.3.5",
    "preact": "^6.0.1",
    "preact-compat": "^3.0.0",
    "preact-markup": "^1.3.0",
    "preact-router": "^2.0.0",
    "preact-svg": "^1.4.3",
    "progress-spinner": "^1.1.0",
    "promise-polyfill": "^6.0.2",
    "proptypes": "^0.14.3",
    "snyk": "^1.17.1",
    "yaml": "^0.3.0"
  },
  "main": "webpack.config.babel.js",
  "directories": {
    "test": "test"
  },
  "snyk": true
}
